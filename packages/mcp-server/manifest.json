{
  "dxt_version": "0.2",
  "name": "profound-mcp",
  "version": "0.5.0",
  "description": "The official MCP Server for the Profound API",
  "author": {
    "name": "Profound",
    "email": "support@tryprofound.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cooper-square-technologies/profound-typescript-sdk.git"
  },
  "homepage": "https://github.com/cooper-square-technologies/profound-typescript-sdk/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.tryprofound.com",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "PROFOUND_API_KEY": "${user_config.PROFOUND_API_KEY}"
      }
    }
  },
  "user_config": {
    "PROFOUND_API_KEY": {
      "title": "api_key",
      "description": "API Key",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
